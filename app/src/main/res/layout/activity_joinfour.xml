<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="joinfourVM" type="com.mtgmtgsapplication.app.modules.joinfour.data.viewmodel.JoinfourVM"/>
    </data>
    <LinearLayout android:id="@+id/linearJoinFour"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearStatusBar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTime"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_38pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_time"
                tools:ignore="ContentDescription" tools:src="@drawable/img_time"/>
            <ImageView android:id="@+id/imageCar"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_225pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_67pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_car"
                tools:ignore="ContentDescription" tools:src="@drawable/img_car"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumntitle"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700_cccornerRadius">
            <TextView android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_73pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{joinfourVM.joinfourModel.txtTitle}"
                style="@style/txtNotoSansKRnotosanskrmedium28" tools:text="@string/lbl19"/>
            <View android:background="@color/black_900_33"
                android:id="@+id/lineVectorForty"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_250pxh"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtFive"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_9pxh"
                android:text="@{joinfourVM.joinfourModel.txtFive}"
                style="@style/txtNotoSansKRnotosanskrmedium19" tools:text="@string/msg_52"/>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl20"
                android:id="@+id/etGroupTwentySix"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@={joinfourVM.joinfourModel.etGroupTwentySixValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etNotoSansKRnotosanskrmedium14" tools:ignore="Autofill"/>
            <EditText android:ellipsize="end"
                android:hint="@string/msg8"
                android:id="@+id/etGroupTwentyEight"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@={joinfourVM.joinfourModel.etGroupTwentyEightValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etNotoSansKRnotosanskrmedium14" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btn"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true" android:text="@string/lbl21"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline" tools:text="@string/lbl21"/>
            <View android:background="@color/black_900_33"
                android:id="@+id/lineVectorFortyOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_48pxv" android:layout_width="@dimen/_250pxh"/>
            <FrameLayout android:id="@+id/frameStackshadow"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_64pxv"
                android:layout_marginBottom="@dimen/_27pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_93pxv" android:layout_width="@dimen/_220pxh">
                <ImageView android:id="@+id/imageShadow"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_shadow"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_shadow"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btn1"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl22"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl22"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
