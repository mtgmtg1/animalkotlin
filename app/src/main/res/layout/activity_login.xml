<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.mtgmtgsapplication.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumndogsonthebea"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackdogsonthebea"
                android:layout_height="@dimen/_387pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageDogsonthebea"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_325pxv"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_dogsonthebea}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_dogsonthebea"/>
                <LinearLayout android:id="@+id/linearColumntime"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700_3f">
                    <androidx.appcompat.widget.Toolbar
                        android:gravity="start"
                        android:id="@+id/toolbarToolbar"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_305pxv"
                        android:layout_width="match_parent"
                        app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
                        <LinearLayout android:id="@+id/linearRowtime"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageTime"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_time"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_time"/>
                            <ImageView android:id="@+id/imageCar"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginStart="@dimen/_225pxh"
                                android:layout_width="@dimen/_67pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_car"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_car"/>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>
                </LinearLayout>
                <FrameLayout
                    android:id="@+id/frameStackpawpetcaretwentyone"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_154pxv"
                    android:layout_marginEnd="@dimen/_110pxh"
                    android:layout_marginStart="@dimen/_110pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_154pxh" style="@style/groupStylewhite_A700cornerRadius">
                    <ImageView
                        android:id="@+id/imagePAwPetCareTwentyOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_127pxh"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_127pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_pawpetcare21}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_pawpetcare21"/>
                </FrameLayout>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtOne"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{loginVM.loginModel.txtOne}"
                style="@style/txtNotoSansKRnotosanskrmedium22" tools:text="@string/lbl"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                android:ellipsize="end" android:hint="@string/lbl2"
                android:id="@+id/etGroupTwentyFour"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_27pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={loginVM.loginModel.etGroupTwentyFourValue}"
                android:textColorHint="@color/black_900_66"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                android:ellipsize="end" android:hint="@string/lbl3"
                android:id="@+id/etGroupTwentyFive"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_27pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={loginVM.loginModel.etGroupTwentyFiveValue}"
                android:textColorHint="@color/black_900_66"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btn"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.52"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl4"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl4"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btn1"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.48"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/lbl5"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl5"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowgrouptwentyone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_yellow_a700_radius_27"
                    android:id="@+id/btnGroupTwentyOne"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group21" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_light_green_500_radius_27"
                    android:id="@+id/btnGroupTwentyTwo"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group22" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_27"
                    android:id="@+id/btnGroupTwentyThree"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:paddingHorizontal="@dimen/_5pxh"
                    android:paddingVertical="@dimen/_5pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group23" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
