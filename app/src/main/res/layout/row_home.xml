<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeRowModel" type="com.mtgmtgsapplication.app.modules.home.data.model.HomeRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearCard"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="horizontal" style="@style/groupStylewhite_1">
        <FrameLayout android:id="@+id/frameStackgrid"
            android:layout_height="@dimen/_24pxv"
            android:layout_marginBottom="@dimen/_66pxv"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_27pxh">
            <ImageView android:id="@+id/imageImageTwo"
