        <TextView android:id="@+id/txtNameFour"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_45pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_weight="0.62" android:layout_width="0dp"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:text="@{homeRowModel.txtNameFour}"
            style="@style/txtNotoSansKRnotosanskrmedium16_2" tools:text="@string/lbl37"/>
    </LinearLayout>
</layout>
